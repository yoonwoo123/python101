    "def my_sqrt(x):\n",
    "    a = x-1\n",
    "    b = x\n",
    "    \n",
    "    while a**2 < x < b**2:\n",
    "        if ((a+b)/2)**2 > x:\n",
    "            b = (a+b)/2\n",
    "        elif ((a+b)/2)**2 < x:\n",
    "             a = (a+b)/2   \n",
    "        elif b <1.5:\n",
    "            return b"
